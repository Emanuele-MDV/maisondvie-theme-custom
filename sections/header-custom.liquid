<a id="siteBranding">
  <figure>
    <img
    src="{{ section.settings.logo | image_url: width: '120x' }}"
    alt="{{ section.settings.logo.alt | default: shop.name | escape }}"
    srcset=""
    sizes=""
    width=""
    height=""
    />
  </figure>
</a>


{% schema %}
  {
    "name": "Customized header",
    "tag":"header",
    "class":"header-custom",
    "settings": [
      {
        "type": "image_picker",
        "id": "logo",
        "label": "t:sections.header.settings.logo.label"
      }
    ]
  }
{% endschema %}